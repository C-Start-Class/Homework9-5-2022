#include <stdio.h>
#include <string.h>
int main()
{
    char str[100];
    char str2[100];
    int j = 0;
    printf("Nhap chuoi: ");
    gets(str);
    for(int i = strlen(str)-1; i >=0;i--){
        str2[j++]=str[i];
    }
    if(!strcmp(str2,str)){
        printf("%s la chuoi palindrome",str);
    }
    else{
        printf("%s khong la chuoi palidrome",str);
    }
}
