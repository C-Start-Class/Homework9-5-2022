#include <stdio.h>
#include <string.h>
int main()
{
    char str[100];
    int count = 0;
    printf("Nhap chuoi: ");
    gets(str);
    for(int i = 0; i < strlen(str);i++){
        if(str[i]=='a'||str[i]=='i'||str[i]=='u'||str[i]=='e'||str[i]=='o'||str[i]=='A'||str[i]=='I'||str[i]=='U'||str[i]=='E'||str[i]=='O'){
            count++;
        }
    }
    printf("So chu cai: %d",count);
}
